{
  "title": "Executive \u2014 Omni/UBL (Latency & Errors)",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "timezone": "browser",
  "panels": [
    {
      "type": "stat",
      "title": "HTTP Error Rate (Gateway/Core) \u2014 5m",
      "gridPos": {
        "h": 4,
        "w": 8,
        "x": 0,
        "y": 0
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "values": false
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(gateway_http_requests_total{code=~\"5..\"}[5m])) / clamp_min(sum(rate(gateway_http_requests_total[5m])), 1) * 100"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 2
        }
      }
    },
    {
      "type": "stat",
      "title": "Backpressure (Gateway+Office) \u2014 5m",
      "gridPos": {
        "h": 4,
        "w": 8,
        "x": 8,
        "y": 0
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "values": false
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(increase(gateway_backpressure_count[5m])) + sum(increase(office_mcp_backpressure_count[5m]))"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none"
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Gateway Latency p95 (seconds)",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 4
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right"
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum(rate(gateway_http_request_duration_seconds_bucket[5m])) by (le))"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Core API DB p95 (seconds)",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 4
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right"
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum(rate(core_db_query_seconds_bucket[5m])) by (le, op))"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    }
  ],
  "templating": {
    "list": []
  },
  "time": {
    "from": "now-6h",
    "to": "now"
  }
}