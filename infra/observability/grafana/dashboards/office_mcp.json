{
  "title": "Office \u2014 MCP Tooling",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "timezone": "browser",
  "panels": [
    {
      "type": "timeseries",
      "title": "Tool Calls per second (by tool)",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "right"
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (tool) (rate(office_mcp_call_total[1m]))"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Tool Call Latency p95 (seconds)",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "right"
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum(rate(office_mcp_call_duration_seconds_bucket[5m])) by (le, tool))"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    },
    {
      "type": "timeseries",
      "title": "WS Reconnect (ms) \u2014 p95",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "right"
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum(rate(office_ws_reconnect_ms_bucket[5m])) by (le))"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms"
        }
      }
    },
    {
      "type": "stat",
      "title": "ErrorToken \u2014 \u00faltimos 5m",
      "gridPos": {
        "h": 4,
        "w": 8,
        "x": 12,
        "y": 8
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "values": false
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(increase(office_mcp_errors_total[5m]))"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none"
        }
      }
    }
  ],
  "templating": {
    "list": []
  },
  "time": {
    "from": "now-6h",
    "to": "now"
  }
}